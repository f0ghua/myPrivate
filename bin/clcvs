#!/bin/sh
find . -name 'CVS' -exec rm -rf '{}' ';'
find . -name 'cvs' -exec rm -rf '{}' ';'
find . -name '.cvsig*' -exec rm -rf '{}' ';'
find . -name '*.bak' -exec rm -rf '{}' ';'
find . -name '*.bk' -exec rm -rf '{}' ';'
find . -name '.cvsignore' -exec rm -rf '{}' ';'
find . -name '.svn' -exec rm -rf '{}' ';'
find . -name '.svnignore' -exec rm -rf '{}' ';'
